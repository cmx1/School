<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.wtu.school.fragment.TwoFragment"
    android:background="#fff">

    <!-- TODO: Update blank fragment layout -->
   <RelativeLayout
       android:layout_width="match_parent"
       android:layout_height="match_parent">
       <LinearLayout
           android:id="@+id/fragment_two_menu1"
           android:layout_width="match_parent"
           android:layout_height="100dp"
           android:gravity="center_vertical|center_horizontal"
           >

           <ImageButton
               android:layout_width="50dp"
               android:layout_height="50dp"
                android:background="@drawable/more_contact"
               android:layout_weight="1"
               android:layout_margin="20dp"
               />
           <ImageButton
               android:layout_width="50dp"
               android:layout_height="50dp"
                android:background="@drawable/more_contact"
               android:layout_weight="1"
               android:layout_margin="20dp"
               />
           <ImageButton
               android:layout_width="50dp"
               android:layout_height="50dp"
                android:background="@drawable/more_contact"
               android:layout_weight="1"
               android:layout_margin="20dp"
               />


       </LinearLayout>
       <LinearLayout
           android:id="@+id/fragment_two_menu2"
           android:layout_below="@id/fragment_two_menu1"
           android:layout_width="match_parent"
           android:layout_height="100dp"
           android:gravity="center_vertical|center_horizontal"
           >

           <ImageButton
               android:layout_width="50dp"
               android:layout_height="50dp"
               android:background="@drawable/more_contact"
               android:layout_weight="1"
               android:layout_margin="20dp"
               />
           <ImageButton
               android:layout_width="50dp"
               android:layout_height="50dp"
               android:background="@drawable/more_contact"
               android:layout_weight="1"
               android:layout_margin="20dp"
               />
           <ImageButton
               android:layout_width="50dp"
               android:layout_height="50dp"
               android:background="@drawable/more_contact"
               android:layout_weight="1"
               android:layout_margin="20dp"
               />


       </LinearLayout>
       <com.wtu.school.customview.XListView
           android:id="@+id/fragment_two_listview"
           android:layout_below="@id/fragment_two_menu2"
           android:layout_width="match_parent"
           android:layout_height="match_parent"></com.wtu.school.customview.XListView>

   </RelativeLayout>

</FrameLayout>
